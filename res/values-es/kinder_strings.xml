    <string name="lockgesture_settings_choose_lock_pattern">Establecer gesto de desbloqueo</string>
    <!-- Security & location settings screen, setting option name when user has previously set an unlock gesture and wants to change to a new pattern -->
    <string name="lockgesture_settings_change_lock_pattern">Cambie gesto de desbloqueo</string>
    <!-- Security & location settings screen, the help instructions (an animation) caption -->
    <string name="lockgesture_settings_help_how_to_record">Cómo dibujar un gesto de desbloqueo</string>
